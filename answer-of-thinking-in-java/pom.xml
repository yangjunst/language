        </dependency>
        <dependency>
            <groupId>com.maomao.learn</groupId>
            <artifactId>apt-mirror-api</artifactId>
            <version>0.1</version>
        </dependency>
        <dependency>
            <groupId>com.maomao.learn</groupId>
